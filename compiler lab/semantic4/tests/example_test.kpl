Program Example2; (* Factorial *)
   
Type T = Integer;
Var n : Integer;
    A : ARRAY(. 2 .) OF ARRAY(. 4 .) Of T;

Function F(var n : Integer) : Integer;
  Begin
    If n = 0 Then F := 1 Else F := N * F (N - 1);
  End;

Begin
  A(.4.)(.2.) := 0;
  For n := 1 To 7 Do
    Begin
      Call WriteLn;
      Call WriteI(F(n));
    End;
End. (* Factorial *)