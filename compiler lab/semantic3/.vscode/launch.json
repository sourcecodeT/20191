{
    "version": "0.2.0",
    "configurations": [{
        "name": "(gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/incompleted/kplc",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/incompleted",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [{
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        }],
        "logging": {
            "engineLogging": true
        }
    }]
}